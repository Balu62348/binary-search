    //* ADC RJY P B SUBRAHMANYAM*//
/*A program move to all zeros at the end*/
#include<stdio.h>
int main()
{
int arr[]={9,0,1,0,2,3,0,0,4,2};
int i,count=0,n=10;
for(i=0;i<n;i++)
{
if (arr[i]>0)
{
arr[count++]=arr[i];
}}
while(count<n){
arr[count++]=0;
}
for(i=0;i<n;i++)
{
printf("%d",arr[i]);
}
return 0;
}
